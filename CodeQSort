#include <iostream>
#include <ctime>
#include<fstream>
using namespace std;

int cmpfunc(const void* a, const void* b)
{
	return (*(float*)a - *(float*)b);
}

int main()
{
	ifstream fi("D:\\Máy tính\\CTDLVaGT_QuaTrinh\\taofiletest\\test10.txt");
	int n;
	fi >> n;
	float* a = new float[n];
	for (int i = 0; i < n; i++)
	{
		fi >> a[i];
	}
	clock_t start, end;
	double cpu_time_used;
	start = clock();
	qsort(a, n, sizeof(int), cmpfunc);
	end = clock();
	cpu_time_used = ((double)(end - start));
	cout << "Time used " << cpu_time_used;
	return 0;
}
